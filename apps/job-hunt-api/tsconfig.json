{
    "compilerOptions": {
        "target": "es2016" ,
        "module": "commonjs" ,
        "rootDir": "./src",
        "outDir": "./dist" ,
        "esModuleInterop": false ,
        "forceConsistentCasingInFileNames": true,
        "strict": true ,
        "skipLibCheck": true
      },
      "exclude": ["node_modules"],
      "include": ["src/**/*"]
}